    <%= form_with model: @procedure, url: admin_procedures_path, local: true, class: "w-96" do |form| %>

        <div class="field">
          <%= form.label :tag, "Tag", value: @procedure.try(:tag), class: "form-label" %><br />
          <%= form.text_field(
            :tag,
            required: true,
            class: "form-text-input text-sm"
          ) %>
        </div>

        <div class="field">
          <%= form.label :tag, "Title", value: @procedure.try(:title), class: "form-label" %><br />
          <%= form.text_field(
            :title,
            required: true,
            class: "form-text-input text-sm"
          ) %>
        </div>

          <div class="Action">
          <%= form.submit :Create, class: 'add-button' %>
      </div>
    <% end %>
    <%= link_to 'Back', admin_procedures_path, class: "text-xs py-8" %>